{
  "name": "hisproblemnow",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build-client": "webpack",
    "build-server": "babel src -d build",
    "build": "npm run clean && npm run build-client && npm run build-server",
    "clean": "rm -rf build",
    "start": "node build/server",
    "dev": "webpack --progress && babel-node src/server",
    "dev:watch:client": "webpack --watch --progress",
    "dev:watch:server": "nodemon --exec babel-node src/server --ignore build --ignore src/client",
    "postinstall": "npm run build",
    "lint": "eslint src",
    "test": "npm run lint"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "bunyan": "^1.8.5",
    "bunyan-middleware": "^0.5.1",
    "express": "~4.13.4",
    "helmet": "^3.1.0",
    "lodash": "^4.17.2",
    "moment": "^2.16.0",
    "phone-formatter": "0.0.2",
    "prismic.io": "^3.3.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "webpack": "^1.13.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/demands/hisproblemnow"
  },
  "engines": {
    "node": "6.9.1"
  },
  "devDependencies": {
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "flow-bin": "^0.36.0",
    "nodemon": "^1.11.0"
  }
}
